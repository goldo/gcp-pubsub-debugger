  "description": "Tool for debugging Google Cloud Platform PubSub",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "index.js",
  "scripts": {
    "start:server": "node server/index",
    "start:client": "FORCE_COLOR=true react-scripts start | cat",
    "build:client": "react-scripts build",
