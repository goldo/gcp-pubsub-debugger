  },
  "dependencies": {
    "@google-cloud/pubsub": "0.18.0",
    "apollo-cache-inmemory": "1.1.12",
    "apollo-client": "2.2.8",
    "apollo-link": "1.2.1",
    "apollo-link-error": "1.0.7",
    "apollo-link-http": "1.5.3",
    "apollo-link-ws": "1.0.7",
    "async": "2.6.0",
    "dotenv": "^5.0.1",
    "graphql": "0.13.2",
    "graphql-tag": "2.8.0",
